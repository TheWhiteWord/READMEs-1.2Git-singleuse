{
    "name": "readmes-extension",
    "displayName": "READMEs",
    "description": "READMEs extension for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "readmes.execute",
                "title": "READMEs: Execute Function"
            },
            {
                "command": "readmes.navigate",
                "title": "READMEs: Navigate Warmhole"
            }
        ]
    },
    "scripts": {
        "test": "node ./test/runTest.js"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "fs-extra": "^10.0.0",
        "vscode": "^1.1.37"
    },
    "devDependencies": {
        "@types/node": "^16.x",
        "@types/vscode": "^1.80.0"
    }
}
